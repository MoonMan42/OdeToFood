@page
@model OdeToFood.Pages.Restaurant.ListModel
@{
    ViewData["Title"] = "Restaurant";
}

<h2>Restaurant</h2>

<table class="table">
    @foreach (var r in Model.Restaurants)
    {
        <tr>
            <td>@r.Name</td>
            <td>@r.Location</td>
            <td>@r.Cuisine</td>
        </tr>
    }
</table>

